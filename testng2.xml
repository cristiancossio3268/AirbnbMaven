<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="AirBnb Suite">
    <test name="Test">
        <groups>
            <run>
                <include name="profile"></include>
                <include name="destinations"></include>
                <exclude name="template"></exclude>
                <include name="setup"></include>
            </run>
        </groups>
        <packages>
            <package name="airbnb"></package>
        </packages>
    </test>
</suite>